{
    "files.exclude": {
        "**/*.cmd": true,
        "**/*.d": true,
        "**/*.o": true,
        "**/powerpc": true,
        "**/arm": true,
        "**/mips": true,
        "**/x86": true,
        "**/.vscode": true,
        "**/compile_commands.json": true,
        "**/.cache": true
    },
    "explorer.excludeGitIgnore": true,
    "C_Cpp.intelliSenseEngine": "Disabled",
    "[c]": {
        "editor.detectIndentation": false,
        "editor.tabSize": 8,
        "editor.insertSpaces": false,
        "editor.rulers": [80,100]
    },
    "files.associations": {
        "*.h": "c"
    }
}
